import math

x = float(input("Nhập x: "))
y = float(input("Nhập y: "))
z = float(input("Nhập z: "))
F = (x + y + z)/(x**2 + y**2 + 1) - abs(x - z * math.cos(y))
print(f"Gia tri cua F = {F}")


import math
x = float(input("Type in a x: "))
y = float(input("Type in a y: "))
z = float(input("Type in a z: "))
a = x+y+z
b = x**2+y**2+1
c = x-z*math.cos(y)
if c < 1:
  c = c*-1
F = (a)/(b) - c
print(f"The value of F is: {F}")
